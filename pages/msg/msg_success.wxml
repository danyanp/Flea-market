<view class="page">
    <view class="weui-msg">
        <!-- icon图标 -->
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>

        <!-- 文字提示 -->
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">发布成功</view>
            <view class="weui-msg__desc">{{data}}</view>
        </view>

        <!-- 按钮 -->
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button style='margin-left:20px;margin-right:20px;' class="weui-btn" type="primary" bindtap="setJump">点击返回</button>
                <!-- <button class="weui-btn" type="default">辅助操作</button> -->
            </view>
        </view>

        <!-- 底部 -->
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                </view>
                <view class="weui-footer__text">Copyright © 2018-2019 超级校园墙</view>
            </view>
        </view>
    </view>
</view>